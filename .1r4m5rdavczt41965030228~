#include <stdio.h>
#include <stdlib.h>

int main()
{
    char sentence[] = "Random text";
    int index;

    index = 0;
    while(sentence[index] != '\0')
    {
        putchar(sentence[index]);
        index++;
    }
    putchar('\n');

    char firstname[16];
    char lastname[16];

    printf("What is your name? ");
    scanf("%15s%15s", firstname, lastname);
    printf("Pleased to meet you,%15s%15s\n",firstname, lastname);
    return(0);
}
